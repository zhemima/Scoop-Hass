    "shortcuts": [
        [
            "assetcheck.exe",
            "assetcheck"
        ]
    ],
    "checkver": {
        "url": "https://upr.unity.cn/backend/documents?type=USER_MANUAL_zh",
        "regex": "Asset Checker V(?<version>[\\d.]+)"
    },
